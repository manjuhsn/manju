pipeline {
  agent any
  stages {
    stage('Build stage') {
      parallel {
        stage('Build stage') {
          steps {
            echo 'Hi hello'
          }
        }
        stage('') {
          steps {
            echo 'Manju'
          }
        }
      }
    }
    stage('build stage') {
      steps {
        echo 'hello world'
      }
    }
  }
}